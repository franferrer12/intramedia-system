{
  "name": "intra-media-backend",
  "version": "1.0.0",
  "description": "API REST para sistema de gesti√≥n de agencia de DJs",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "migrate:excel": "node src/utils/migrateFromExcel.js",
    "migrate:real": "node src/utils/migrateRealData.js",
    "db:setup": "psql -U postgres -c 'CREATE DATABASE intra_media_system;' && npm run db:migrate",
    "db:migrate": "psql -U postgres -d intra_media_system -f ../database/schema.sql",
    "test": "node --test tests/*.test.js",
    "test:unit": "node --test tests/schemas.test.js tests/validate.test.js",
    "test:middleware": "node --test tests/cache.test.js tests/compression.test.js tests/performance.test.js tests/rateLimit.test.js tests/security.test.js",
    "test:integration": "node --test tests/integration.*.js",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:watch": "node --test --watch tests/",
    "test:coverage": "c8 --reporter=text --reporter=html --reporter=lcov node --test tests/*.test.js",
    "test:coverage:report": "c8 report"
  },
  "keywords": [
    "djs",
    "eventos",
    "gestion",
    "api"
  ],
  "author": "Intra Media",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "bull": "^4.16.5",
    "cheerio": "^1.1.2",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "googleapis": "^166.0.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "passport": "^0.7.0",
    "passport-instagram": "^1.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "playwright": "^1.56.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "redis": "^5.10.0",
    "stripe": "^20.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.6",
    "winston": "^3.18.3",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "artillery": "^2.0.21",
    "c8": "^10.1.3",
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.4"
  }
}
